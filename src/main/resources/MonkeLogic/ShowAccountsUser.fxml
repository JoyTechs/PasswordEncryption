            xmlns:fx="http://javafx.com/fxml/1" fx:controller="MonkeLogic.methods.ShowAccountsUserFrontEnd">

   <children>
      <TableView fx:id="accountsTable" layoutY="60.0" prefHeight="300.0" prefWidth="600.0">
         <columns>
