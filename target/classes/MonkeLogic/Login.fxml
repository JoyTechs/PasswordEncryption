<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="MonkeLogic.methods.LoginFrontEnd">


    <Text fx:id="text1" layoutX="20" layoutY="30" stroke="black" text="Welcome">
        <font>
            <Font name="Arial" size="32.0" />
        </font>
    </Text>

    <Label fx:id="userNameLabel" layoutX="148.0" layoutY="156.0" text="Username: " />
    <TextField fx:id="usernameInpt" layoutX="217.0" layoutY="152.0" />

    <Label fx:id="passwordLabel" layoutX="150.0" layoutY="204.0" text="Password: " />
    <PasswordField fx:id="passwordInpt" layoutX="217.0" layoutY="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />

    <Button fx:id="login" layoutX="268.0" layoutY="252.0" text="Login" onAction="#logintest"/>


</AnchorPane>
